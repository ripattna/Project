   "source": [
    "import multiprocessing as mp\n",
    "\n",
    "\n",
    "def sqr(x, y):\n",
    "    y.put(x * x)\n",
    "\n",
    "\n",
    "if __name__ == '__main':\n",
    "    y = mp.Queue()\n",
    "    p = mp.Process(target=sqr, args=(4, y))\n",
    "    p.start()\n",
    "    p.join()\n",
    "\n",
    "    result = y.get()\n",
    "    print(result)"
   ]
